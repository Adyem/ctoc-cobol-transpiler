file in "input.txt";
file out "filtered.txt";
char line[128];

function filter_prefix() {
    open(in, "r");
    open(out, "w");
    while (read(in, line)) {
        if (starts_with(line, "ERR")) {
            write(out, line);
        }
    }
    close(in);
    close(out);
}

function main() {
    filter_prefix();
}
