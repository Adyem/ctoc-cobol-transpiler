function MAIN() {
    open(INPUT_FILE, "r");
    while (!(EOF_FLAG == true)) {
        read(INPUT_FILE, OUTPUT_RECORD);
        if (EOF_FLAG == false) {
            write(OUTPUT_FILE, OUTPUT_RECORD);
        } else {
            EOF_FLAG = true;
        }
    }
    close(INPUT_FILE);
    return ;
}
